---
import { Image } from "astro:assets";
import KMUTT_Logo from "../../assets/logo/kmutt+65+eng+smo.webp";

const { ...attrs } = Astro.props;
---

<Image
  src={KMUTT_Logo}
  alt="KMUTT Logo"
  class="px-6 sm:px-0 select-none"
  width={360}
  height={360}
  sizes="360px"
  format="webp"
  loading="eager"
  decoding="async"
  fetchpriority="high"
  data-fade
  {...attrs}
/>
