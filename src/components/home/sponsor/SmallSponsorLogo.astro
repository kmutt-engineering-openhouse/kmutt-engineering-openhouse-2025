---
import { Image } from "astro:assets";
import Small_Spon_Logo from "../../../assets/logo/sponsor/small/Website-Small-Logo-Spon-OPH.svg";

interface Props {
  class?: string;
  [key: string]: any;
}

const { class: className = "", ...attrs } = Astro.props;
---

<Image
  src={Small_Spon_Logo}
  alt="Small Sponsor Logo"
  class={`px-6 select-none object-cover sm:px-0 ${className}`}
  width={460}
  sizes="(max-width: 360px) 100vw, 460px"
  loading="lazy"
  fetchpriority="high"
  format="svg"
  quality={100}
  {...attrs}
/>
